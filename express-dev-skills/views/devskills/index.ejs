<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Developer Skills</title>
  </head>
  <body>
    <h1>Dev Skills</h1>
    <ul>
      <% devskills.forEach(function(s) { %>
      <li>
        <a href="/skills/<%= s.id %>"><%= s.skill %></a>
        <%= s.skill %> - <%= s.learned ? 'learned' : 'not learned' %>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
